#Indonesia Flag 
from turtle import*
shape("turtle")
turtlesize(5)
fillcolor("#ff0000")
begin_fill()
penup()
forward(520)
pendown()
left(90)
forward(310)
left(90)
forward(1040)
left(90)
forward(620)
left(90)
forward(1040)
left(90)
forward(310)
end_fill()
fillcolor("#FFFFFF")
begin_fill()
left(90)
forward(1040)
left(90)
forward(310)
left(90)
forward(1040)
left(90)
forward(310)
end_fill()
hideturtle()
done() 